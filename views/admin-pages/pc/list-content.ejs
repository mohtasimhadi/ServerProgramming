<div class="right_col" role="main">
    <div class="col-md-12 col-sm-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>Math Olympiad<small>Participants List</small></h2>
                <ul class="nav navbar-right panel_toolbox">
                    <li>
                        <a class="collapse-link"
                            ><i class="fa fa-chevron-up"></i
                        ></a>
                    </li>
                </ul>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="card-box table-responsive">
                            <table
                                id="datatable-responsive"
                                class="
                                    table table-striped table-bordered
                                    dt-responsive
                                    nowrap
                                "
                                cellspacing="0"
                                width="100%"
                            >
                                <thead>
                                    <tr>
                                        <th>Team Name</th>
                                        <th>Institution</th>
                                        <th>Coach Name</th>
                                        <th>Leader Name</th>
                                        <th>Member 1 Name</th>
                                        <th>Member 2 Name</th>
                                        <th>Total</th>
                                        <th>Paid</th>
                                        <th>Selected</th>
                                        <th>Payment<br />Status</th>
                                        <th>Select</th>
                                        <th>Payment<br />Done</th>
                                        <th>Edit</th>
                                        <th>Delete</th>
                                    </tr>
                                </thead>

                                <tbody>
                                    <% if(programmingTeam.length>0){
                                    programmingTeam.forEach(programmingTeam=>{%>
                                    <tr>
                                        <td><%= programmingTeam.teamName %></td>
                                        <td>
                                            <%= programmingTeam.institutionName
                                            %>
                                        </td>
                                        <td>
                                            <%= programmingTeam.coachName %>
                                        </td>
                                        <td>
                                            <%= programmingTeam.leaderName %>
                                        </td>
                                        <td>
                                            <%= programmingTeam.member1Name %>
                                        </td>
                                        <td>
                                            <%= programmingTeam.member2Name %>
                                        </td>
                                        <td><%= programmingTeam.total %></td>
                                        <td><%= programmingTeam.paid %></td>
                                        <td>
                                            <%
                                            if(programmingTeam.selected==true){%>
                                            <p style="color: green">Yes</p>
                                            <%}else{%>
                                            <p style="color: red">No</p>
                                            <%}%>
                                        </td>
                                        <td>
                                            <%
                                            if(programmingTeam.paid==programmingTeam.total){%>
                                            <p style="color: green">
                                                Successful
                                            </p>
                                            <%}else{%>
                                            <p style="color: red">Incomplete</p>
                                            <%}%>
                                        </td>
                                        <td>
                                            <% const
                                            selectURL="select/"+programmingTeam._id%>
                                            <a href="<%= selectURL %>">
                                                <i class="fa fa-check"></i>
                                                <span
                                                    class="fa fa-chevron"
                                                ></span>
                                            </a>
                                        </td>
                                        <td>
                                            <% const
                                            paymentDoneURL="paymentDone/"+programmingTeam._id%>
                                            <a href="<%= paymentDoneURL %>">
                                                <i class="fa fa-money"></i>
                                                <span
                                                    class="fa fa-chevron"
                                                ></span>
                                            </a>
                                        </td>
                                        <td>Edit</td>
                                        <td>
                                            <% const
                                            deleteURL="delete/"+programmingTeam._id%>
                                            <a href="<%= deleteURL %>">
                                                <i class="fa fa-trash"></i>
                                                <span
                                                    class="fa fa-chevron"
                                                ></span>
                                            </a>
                                        </td>
                                    </tr>
                                    <% }) } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>